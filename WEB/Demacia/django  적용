1. pip install pep8
2. pip install --upgrade autopep8
3. pip install yapf


pip install django
django-admin startproject gamebotproj
cd .\web_proj\
python manage.py runserver
python manage.py startapp gamebotapp


# db에 python으로 작성한 db구조를 적용하는 명령어
python manage.py makemigrations
python manage.py migrate

# db에 있는 구조를 python으로 옮겨오는 명령어
python manage.py inspectdb 

